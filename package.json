  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crock/netlify-functions-discord-utils.git"
  },
  "bugs": {
    "url": "https://github.com/crock/netlify-functions-discord-utils/issues"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "discord.js": "^12.0.1"
  },
  "devDependencies": {
    "@netlify/zip-it-and-ship-it": "^0.3.1",
    "netlify-lambda": "^1.6.3",
    "npm-install-peers": "^1.2.1",
    "prettier": "^1.19.1",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "@discordjs/opus": "^0.1.0",
    "bufferutil": "^4.0.1",
    "erlpack": "discordapp/erlpack",
    "libsodium-wrappers": "^0.7.6",
    "node-opus": "^0.3.1",
    "opusscript": "^0.0.6",
    "sodium": "^3.0.2",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.6",
    "ffmpeg-static": "^2.4.0"
  },
  "scripts": {
    "start": "netlify-lambda serve src/functions --config webpack.functions.js",
    "build": "netlify-lambda build src/functions --config webpack.functions.js",
    "build-archive": "node index.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "postinstall": "npm-install-peers && netlify-lambda install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "engineStrict": true
}
